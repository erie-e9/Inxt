{
  "name": "nextjs-boilderplate",
  "version": "1.0.0",
  "description": "A practical and complete boilderplate powered with NextJS + TypeScript. A bundle based on PWA + SSR + GraphQL + I18N etc...",
  "scripts": {
    "dev": "next",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "yarn build && cross-env NODE_ENV=production node dist/index.js",
    "ewe": "yarn build && nodemon server/index.ts",
    "nextexp": "next build && next export",
    "iconsgen": "real-favicon generate faviconDescription.json faviconData.json public/images/icons",
    "type-check": "tsc",
    "svg-to-tsx": "svgr < public/images/svgr/wave.svg > components/commons/svgr/wave.tsx --svgo-config public/svgo.json",
    "analyze": "cross-env ANALYZE=true yarn build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser yarn build"
  },
  "keywords": [
    "NextJS",
    "TypeScript",
    "PWA",
    "SSR",
    "GraphQL",
    "I18N",
    "Styled-Components",
    "Antd"
  ],
  "author": "Eric Torres Andrade <erictorresandrade.1@gmail.com>",
  "dependencies": {
    "@next/bundle-analyzer": "^9.1.5",
    "@svgr/cli": "^4.3.3",
    "@types/countdown": "^0.0.7",
    "@types/d3": "^5.7.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.26.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "countdown": "^2.6.0",
    "d3": "^5.14.2",
    "framer-motion": "^1.6.17",
    "glob": "^7.1.6",
    "intersection-observer": "^0.7.0",
    "less": "^3.10.3",
    "less-vars-to-js": "^1.3.0",
    "mime": "^2.4.4",
    "next": "latest",
    "next-seo": "^3.1.0",
    "node-sass": "^4.13.1",
    "nprogress": "^0.2.0",
    "null-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-image-webp": "^0.6.0",
    "react-intersection-observer": "^8.25.2",
    "react-player": "^1.14.2",
    "react-slick": "^0.25.2",
    "react-spring": "^8.0.27",
    "sass-loader": "^8.0.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.16",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^6.0.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "next-purgecss": "^3.1.2",
    "purgecss-webpack-plugin": "^1.6.0",
    "style-loader": "^1.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-ant-icon-loader": "^1.0.8",
    "webpack-cli": "^3.3.10",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "license": "ISC"
}
